N=5

      1
     1  1
   1  2   1
 1   3   3   1
1  4   6   4   1

ArrayList<List<Integer> pascal(int n)
{
    ans;
    prev =null // arraylist
    
    
    for(int i=0; i<n; i++)
    {
    
    curr // arraylist
    
    
    for(int j=0; j<=i; j++)
        if(j==0 || j==i)
        curr.add(1);
        else
        curr.add(prev-> j-1 + prev -> j)
        
    prev= curr;
    ans--> add(curr)
    
    }

}
